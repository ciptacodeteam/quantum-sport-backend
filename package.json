{
  "name": "quantum-sport-backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot serve.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "bun run ./dist/serve.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "vitest run",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:fresh": "prisma db push --force-reset && tsx prisma/seed.ts",
    "db:pull": "prisma db pull",
    "db:truncate": "tsx prisma/truncate.ts",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit",
    "vercel:deploy": "vercel",
    "vercel:dev": "vercel dev",
    "vercel:prod": "vercel --prod"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.5",
    "@hono/zod-validator": "^0.7.4",
    "@prisma/client": "^6.17.1",
    "argon2": "^0.44.0",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.3",
    "hono": "^4.9.12",
    "hono-pino": "^0.10.3",
    "http-status": "^2.1.0",
    "mime-types": "^3.0.1",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "sharp": "^0.34.4",
    "slugify": "^1.6.6",
    "xendit-node": "^7.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.8.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vercel": "^48.6.0",
    "vitest": "^3.2.4"
  }
}
