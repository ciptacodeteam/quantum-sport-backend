{
  "name": "quantum-sport-backend",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "format": "prettier -w .",
    "test": "vitest run",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:fresh": "prisma db push --force-reset && tsx prisma/seed.ts",
    "db:pull": "prisma db pull",
    "db:truncate": "tsx prisma/truncate.ts",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.5",
    "@hono/zod-openapi": "^1.1.3",
    "@hono/zod-validator": "^0.7.4",
    "@prisma/client": "^6.17.1",
    "@scalar/hono-api-reference": "^0.9.21",
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "dayjs-plugin-duration": "^0.0.1-security",
    "dotenv": "^17.2.3",
    "hono": "^4.9.12",
    "hono-pino": "^0.10.3",
    "http-status": "^2.1.0",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "^1.3.0",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vitest": "^3.2.4"
  }
}
